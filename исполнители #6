from turtle import *

tracer(0)
r = 50
for i in range(8):
    for j in range(4):
        fd(5*r)
        rt(30)
        fd(6*r)
        rt(150)
    rt(60)

up()
for x in range(-30, 30):
    for y in range(-30, 30):
        goto(x * r, y * r)
        dot(4, 'red')
update()
exitonclick()
screensize(10000, 10000)
